    <script type="text/javascript">
    function Logoff() {
        setTimeout(function () {
            window.location.href = '/ads/Other/Logout';
        }, 500);
    }
    var __showContextSwitch__ = function (data) {
            $a.ui.simple.confirm(
                'Switch Account',
                "<label>You will be logged into:</label>"
                + "<dl class='inline-block'>"
                + "<dt>Organization Code</dt><dd>" + data.userLinkCode + "</dd>"
                + "<dt>Organization Name</dt><dd>" + data.userLinkName + "</dd>"
                + (data.userLinkRole != '' ? "<dt>Role</dt><dd>" + data.userLinkRole + "</dd>" : "")
                + "</dl>",
                //+ "<label class='checkbox'><input id='__defaultOrg__' type='checkbox'>Set as default</label>",
                null,
                function () {
        //if (document.getElementById('__defaultOrg__').checked) {
        //    data.userLinkHref = data.userLinkHref + "&setToDefault=true";
        //}
        window.location.href = data.userLinkHref;
    },
                'Confirm',
                null,
                null,
                true
            );
        };
    $(function () {
        $(".user-switch a:link").on('click', function (e) {
        e.preventDefault();
        var data = {
                userLinkHref: $(this).attr("href"),
                userLinkRole: $(this).attr("data-role"),
                userLinkCode: $(this).attr("data-orgCode"),
                userLinkName: $(this).attr("data-orgName")
            };
    __showContextSwitch__(data);
        });
        $(".user-switch a:link").on('mousedown', function (e) {
        if (e.which === 2) {
            e.preventDefault();
            var data = {
                    userLinkHref: $(this).attr("href"),
                    userLinkRole: $(this).attr("data-role"),
                    userLinkCode: $(this).attr("data-orgCode"),
                    userLinkName: $(this).attr("data-orgName")
                };
        __showContextSwitch__(data);
        return false;
    }
        });
    });


    </script>                    <div id="no-top-navigation-buffer">&nbsp;</div>
